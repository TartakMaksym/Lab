def input_data():
    symbol = input("Введіть символ: ")
    return symbol

def output_data(result):
    if result == 1:
        print("Отриманий символ є великою літерою англійської абетки.")
    else:
        print("Отриманий символ не є великою літерою англійської абетки.")

def is_upper_case(symbol):
    if symbol.isupper():
        return 1
    else:
        return 0

def main():
    symbol = input_data()

    result = is_upper_case(symbol)

    output_data(result)

if __name__ == "__main__":
    main()
